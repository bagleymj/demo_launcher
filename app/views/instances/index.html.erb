<table>
  <tr>
    <th>Instance Name</th>
    <th>Instance ID</th>
    <th>Stack Name</th>
  </tr>
  <% @instances.each do |instance| %>
    <tr>
      <td><%= instance.instance_name %></td>
      <td><%= instance.instance_id %></td>
      <td><%= instance.stack.stack_name %></td>
    </tr>
  <% end %>
</table>

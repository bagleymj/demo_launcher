<table class="table table-striped">
  <tr>
    <th>
      Logical Resource ID
    </th>
    <th>
      Physical Resource ID
    </th>
    <th>
      Resource Type
    </th>
  </tr>
  <% @resources.each do |resource| %>
    <tr>
      <td>
        <%= resource.logical_resource_id %>
      </td>
      <td>
        <%= resource.physical_resource_id %>
      </td>
      <td>
        <%= resource.resource_type %>
      </td>
    </tr>
  <% end %>
</table>
<h2>Volumes</h2>
<% @standard_volumes.each do |volume| %>
  <div>
    <%= volume %>
  </div>
<%end%>
